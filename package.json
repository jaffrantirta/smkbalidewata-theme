{
  "name": "lbl",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run dev:css & npm run dev:js",
    "dev:css": "postcss src/css/tailwind.css -o assets/css/tailwind.css --watch",
    "dev:js": "webpack --mode=development --watch",
    "build:dev": "npm run build:dev:css && npm run build:dev:js",
    "build:dev:css": "postcss src/css/tailwind.css -o assets/css/tailwind.css",
    "build:dev:js": "webpack --mode=development",
    "build": "npm run build:css && npm run build:js",
    "build:css": "tailwindcss -i src/css/tailwind.css -o assets/css/tailwind.min.css --minify",
    "build:js": "NODE_ENV=production webpack --mode=production"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^7.0.7",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-loader": "^8.1.1",
    "tailwindcss": "^3.4.17",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "photoswipe": "^5.4.4",
    "swiper": "^11.2.10"
  }
}
